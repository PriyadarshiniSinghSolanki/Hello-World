	 input1++;
	 while (*input1 == ']') {
	 arr[i] = (char *) malloc(3 * sizeof(char) + 1);
	 strcpy(*arr, input1);
	 len = strlen(arr);
	 input1 = input1 + len;
	 }

	 } else if (*input1 == '{') {
	 input1++;
