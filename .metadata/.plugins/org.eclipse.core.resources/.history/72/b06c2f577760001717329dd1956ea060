#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

	/* Enter your code here. Read input from STDIN. Print output to STDOUT */
	int T;
	scanf("%d", &T);
	char *s;
	int i = 0;
	for (i = 0; i < T; i++) {
		char *str1, *str2;
		scanf("%s", &s);
		while (*s != '\0') {
			if ((i % 2) == 0) {
				*str1 = *s;
				s++;
				str1++;
				i++;
			} else {
				*str2 = *s;
				s++;
				str2++;
				i++;
			}

		}
		printf("%s %s", str1, str2);

	}

	return 0;
}

