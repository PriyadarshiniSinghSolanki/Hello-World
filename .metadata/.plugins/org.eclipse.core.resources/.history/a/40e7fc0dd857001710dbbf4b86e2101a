#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct structure{
	int SR;
	int rollNo;
	char *name;
}Student;

int main(){
	strcpy(Student->name, "Priyadarshini");
	Student.SR = 1;
	Student.rollNo = 101;

	printf("%d %d %s", Student.SR, Student.rollNo,Student->name);
	return 0;
}
